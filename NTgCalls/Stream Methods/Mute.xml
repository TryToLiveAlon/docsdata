<page>
    <h1>Mute</h1>
    <text>This method allows you to mute the WebRTC stream.</text>
    <h3>Example</h3>
    <syntax-highlight mark="7-9">
        from ntgcalls import NTgCalls
        ...

        app = NTgCalls()
        ...

        app.mute(
            -1001185324811,
        )
    </syntax-highlight>
    <separator/>
    <h2>Details</h2>
    <category-title noref="true">
        <ref>NTgCalls.<sb>mute</sb></ref>()
    </category-title>
    <subtext>
        <text>Mute the WebRTC stream.</text>
        <br/>
        <category>
            <pg-title>PARAMETERS</pg-title>
            <subtext>
                <config id="ARG_INT_ID"/>
            </subtext>
        </category>
        <br/>
        <category>
            <pg-title>RAISES</pg-title>
            <subtext>
                <config id="NTG_NO_GROUP_CALL"/>
            </subtext>
        </category>
    </subtext>
</page>
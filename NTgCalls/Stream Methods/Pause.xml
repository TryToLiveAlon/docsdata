<page>
    <h1>Pause</h1>
    <text>This method allows you to pause the WebRTC stream.</text>
    <h3>Example</h3>
    <syntax-highlight mark="7-9">
        from ntgcalls import NTgCalls
        ...

        app = NTgCalls()
        ...

        app.pause(
            -1001185324811,
        )
    </syntax-highlight>
    <separator/>
    <h2>Details</h2>
    <category-title noref="true">
        <ref>NTgCalls.<sb>pause</sb></ref>()
    </category-title>
    <subtext>
        <text>Pause the WebRTC stream.</text>
        <br/>
        <category>
            <pg-title>PARAMETERS</pg-title>
            <subtext>
                <config id="ARG_INT_ID"/>
            </subtext>
        </category>
        <br/>
        <category>
            <pg-title>RAISES</pg-title>
            <subtext>
                <config id="NTG_NO_GROUP_CALL"/>
            </subtext>
        </category>
    </subtext>
</page>
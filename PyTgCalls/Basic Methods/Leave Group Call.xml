    <syntax-highlight mark="10-12">
        from pytgcalls import Client
        from pytgcalls import idle
        ...

        app = PyTgCalls(client)
        app.start()

        ...  # Call API methods

        app.leave_group_call(
            -1001185324811,
        )

        idle()
